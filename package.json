{
  "type": "module",
  "name": "next-template",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "npx typesync",
    "predev": "yarn run clean:cache && yarn run generate:typeHelper",
    "dev": "NODE_OPTIONS='--inspect' NODE_ENV=development next dev",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "NODE_ENV=production ANALYZE=true yarn run build",
    "check:unusedModules": "npx depcheck --ignores='tslib,dotenv,prettier,@types/*,@babel/*,@typescript-eslint/eslint-plugin,eslint-plugin*'",
    "check:outdatedModules": "yarn outdated",
    "generate:typeHelper": "ts-node -P tsconfig.json -r tsconfig-paths/register src/script/typeHelper.ts",
    "postgenerate:typeHelper": "prettier --write src/type/assetTypeHelper.ts",
    "clean:cache": "rm -rf dist",
    "clean": "yarn run clean:cache && rm -rf node_modules",
    "postclean": "yarn",
    "storybook": "npx msw init public/ && start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@next/font": "^13.2.4",
    "@tanstack/query-core": "4.27.0",
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-query-devtools": "^4.28.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ms": "^2.1.3",
    "next": "^13.2.4",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "polished": "^4.2.2",
    "postinstall": "^0.8.0",
    "prettier": "^2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-use": "^17.4.0",
    "recoil": "^0.7.7",
    "styletron-engine-atomic": "^1.5.0",
    "styletron-react": "^6.1.0",
    "typescript": "^5.0.2",
    "webpack-spritesmith": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@types/babel__core": "^7.20.0",
    "@types/babel__preset-env": "^7.9.2",
    "@types/eslint": "^8.21.3",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/ms": "^0.7.31",
    "@types/prettier": "^2.7.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/styletron-engine-atomic": "^1.1.1",
    "@types/styletron-react": "^5.0.3",
    "ts-node": "^10.9.1"
  }
}
